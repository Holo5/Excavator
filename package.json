{
  "name": "holo5-excavator",
  "version": "0.0.1",
  "description": "Habbo furniture & figure extractor for Holo5.",
  "main": "index.js",
  "scripts": {
    "client:dev:win": "yarn parcel Client\\index.html --no-cache --out-dir Client\\Build",
    "client:dev:unix": "yarn parcel Client/index.html --no-cache --out-dir Client/Build",
    "server:dev": "yarn ts-node-dev --respawn --transpileOnly --poll Server/main.ts",
    "lint": "eslint --ext .js,.ts ./ --color"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pi-Bouf/Holo5-Excavator.git"
  },
  "keywords": [
    "holo5",
    "createjs",
    "habbo",
    "extractor"
  ],
  "author": "Pi-Bouf",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Pi-Bouf/Holo5-Excavator/issues"
  },
  "homepage": "https://github.com/Pi-Bouf/Holo5-Excavator#readme",
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js",
    "mixins": "./Client/assets/sass/mixins.scss",
    "images": "./Client/assets/images/",
    "perfect-scrollbar": "./node_modules/vue2-perfect-scrollbar/dist/vue2-perfect-scrollbar.css"
  },
  "devDependencies": {
    "@types/download": "^6.2.4",
    "@types/express": "^4.17.6",
    "@types/fs-extra": "^9.0.1",
    "@types/listr": "^0.14.2",
    "@types/node": "^14.0.14",
    "@types/vue": "^2.0.0",
    "@types/ws": "^7.2.4",
    "@types/xml2js": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.4.0",
    "@vue/component-compiler-utils": "^3.1.2",
    "eslint": "^7.3.1",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-plugin-import": "^2.20.1",
    "parcel": "^1.12.4",
    "sass": "^1.26.5",
    "ts-node": "^8.10.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.9.3",
    "vue-class-component": "^7.2.3",
    "vue-hot-reload-api": "^2.3.4",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "download": "^8.0.0",
    "express": "^4.17.1",
    "fs": "^0.0.1-security",
    "fs-extra": "^9.0.0",
    "listr": "^0.14.3",
    "listr-simple-tree-renderer": "^0.1.3",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.13",
    "spritesheet-js": "^1.2.6",
    "tsyringe": "^4.3.0",
    "vue": "^2.6.11",
    "vue2-perfect-scrollbar": "^1.5.0",
    "vuex": "^3.4.0",
    "ws": "^7.3.0",
    "xml-js": "^1.6.11",
    "xml2js": "^0.4.23"
  }
}
